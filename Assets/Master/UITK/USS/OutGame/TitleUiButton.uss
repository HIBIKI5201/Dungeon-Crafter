.title-ui-button {
    height: 100px;
    width: 400px;
    font-size: 50px;
    -unity-font-style: bold;
    transition-timing-function: ease-out;
    transition-duration: 0.2s;
    background-color: rgb(0, 0, 0);
    color: rgb(255, 255, 255);
}

.title-ui-button:hover {
    scale: 1.1 1.1;
    transition-timing-function: ease-out;
    transition-duration: 0.2s;
}
